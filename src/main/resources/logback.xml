<configuration>
  <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%highlight(%-5level) %cyan(%logger) - %msg %n</pattern>
    </encoder>
  </appender>

  <logger name="com.redis.RedisClient" level="OFF" />

  <root level="DEBUG">
    <appender-ref ref="STDOUT" />
  </root>
</configuration>
